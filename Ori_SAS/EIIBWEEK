//CREATE   EXEC PGM=IEFBR14
//CRT01    DD DSN=SAP.PIBB.ELIAB.TEXT,
//             DISP=(NEW,CATLG,DELETE),
//             DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//             SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//*
//SAS609  EXEC SAS609,REGION=8M,WORK='120000,8000'
//WALALW    DD DSN=SAP.PIBB.FISS.TXT(0),DISP=SHR
//ALW       DD DSN=SAP.PIBB.RDAL1,DISP=OLD
//ALWZ      DD DSN=&&RDALZ,DISP=(NEW,DELETE,DELETE),
//             DCB=(RECFM=FS,LRECL=27648,BLKSIZE=27648),
//             SPACE=(CYL,(1,1),RLSE),UNIT=SYSDA
//RDAL      DD DSN=SAP.PIBB.FISS.RDAL,DISP=(NEW,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=6320),
//            SPACE=(TRK,(10,5)),UNIT=SYSDA
//NSRS      DD DSN=SAP.PIBB.NSRS.RDAL,DISP=(NEW,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=6320),
//            SPACE=(TRK,(10,5)),UNIT=SYSDA
//LOAN      DD DSN=SAP.PIBB.MNILN(0),DISP=SHR
//NID       DD DSN=SAP.PIBB.RNID.SASDATA,DISP=SHR
//BNMK      DD DSN=SAP.PIBB.KAPITI.SASDATA,DISP=SHR
//DEPOSIT   DD DSN=SAP.PIBB.MNITB(0),DISP=SHR
//FD        DD DSN=SAP.PIBB.MNIFD(0),DISP=SHR
//CISDP     DD DSN=SAP.PBB.CISBEXT.DP,DISP=SHR
//BNMTBL1   DD DSN=SAP.PIBB.KAPITI1(0),DISP=SHR
//BNMTBL3   DD DSN=SAP.PIBB.KAPITI3(0),DISP=SHR
//UMA       DD DSN=SAP.PIBB.UMA.DAILY(0),DISP=SHR
//PGM       DD DSN=SAP.BNM.PROGRAM,DISP=SHR
//ELIAB     DD DSN=SAP.PIBB.ELIAB.TEXT,DISP=MOD
//SFTP01    DD DSN=&&FTPPUT,DISP=(NEW,PASS),
//             SPACE=(TRK,(1)),UNIT=SYSDA,RECFM=FB,LRECL=80
//SASLIST   DD SYSOUT=(,),OUTPUT=(*.PRINT1)
//SYSIN     DD *

OPTIONS SORTDEV=3390 YEARCUTOFF=1950 LS=132 PS=60 NOCENTER;

%INC PGM(EIGWRD1W);

DATA REPTDATE (KEEP=REPTDATE);
  SET DEPOSIT.REPTDATE;
  SELECT(DAY(REPTDATE));
    WHEN (8)  DO; SDD = 1;  WK = '1'; WK1 = '4'; END;
    WHEN(15)  DO; SDD = 9;  WK = '2'; WK1 = '1'; END;
    WHEN(22)  DO; SDD = 16; WK = '3'; WK1 = '2'; END;
    OTHERWISE DO; SDD = 23; WK = '4'; WK1 = '3'; END;
  END;
  MM = MONTH(REPTDATE);
  IF WK = '1' THEN DO;
     MM1 = MM - 1;
     IF MM1 = 0 THEN MM1 = 12;
  END;
  ELSE MM1 = MM;
  SDATE = MDY(MM,SDD,YEAR(REPTDATE));
  SDESC = 'PUBLIC ISLAMIC BANK BERHAD';
  CALL SYMPUT('NOWK',PUT(WK,$1.));
  CALL SYMPUT('NOWK1',PUT(WK1,$1.));
  CALL SYMPUT('REPTMON',PUT(MM,Z2.));
  CALL SYMPUT('REPTMON1',PUT(MM1,Z2.));
  CALL SYMPUT('REPTYEAR',PUT(REPTDATE,YEAR4.));
  CALL SYMPUT('REPTDAY',PUT(DAY(REPTDATE),Z2.));
  CALL SYMPUT('RDATE',PUT(REPTDATE,DDMMYY8.));
  CALL SYMPUT('ELDATE',PUT(REPTDATE,Z5.));
  CALL SYMPUT('SDATE',PUT(SDATE,DDMMYY8.));
  CALL SYMPUT('SDESC',PUT(SDESC,$26.));
RUN;

LIBNAME BNM  "SAP.PIBB.D&REPTYEAR" DISP=OLD;
LIBNAME BNM1 "SAP.PIBB.SASDATA" DISP=SHR;

DATA _NULL_;
  SET ALW.REPTDATE(OBS=1);
  CALL SYMPUT('ALW', PUT(REPTDATE, DDMMYY8.));
RUN;

DATA _NULL_;
  SET LOAN.REPTDATE(OBS=1);
  CALL SYMPUT('LOAN', PUT(REPTDATE, DDMMYY8.));
RUN;

DATA _NULL_;
  SET DEPOSIT.REPTDATE(OBS=1);
  CALL SYMPUT('DEPOSIT', PUT(REPTDATE, DDMMYY8.));
RUN;

DATA _NULL_;
  SET FD.REPTDATE(OBS=1);
  CALL SYMPUT('FD', PUT(REPTDATE, DDMMYY8.));
RUN;

DATA _NULL_;
  INFILE BNMTBL1 OBS=1;
  INPUT @1 REPTDATE YYMMDD8.;
  CALL SYMPUT('KAPITI1', PUT(REPTDATE, DDMMYY8.));
RUN;

DATA _NULL_;
  INFILE BNMTBL3 OBS=1;
  INPUT @1 REPTDATE YYMMDD8.;
  CALL SYMPUT('KAPITI3', PUT(REPTDATE, DDMMYY8.));
RUN;

%MACRO PROCESS;
  %IF "&ALW"="&RDATE" AND
      "&LOAN"="&RDATE" AND
      "&FD"="&RDATE" AND
      "&DEPOSIT"="&RDATE" AND
      "&KAPITI1"="&RDATE" AND
      "&KAPITI3"="&RDATE" %THEN %DO;

      /*** REPORT ON DOMESTIC ASSETS AND LIABILITIES ***/
      %INC PGM(WALWPBBP);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;
      PROC DATASETS LIB=BNM NOLIST;
        DELETE ALWGL&REPTMON&NOWK
               ALWJE&REPTMON&NOWK;
      RUN;
      %INC PGM(DALWPBBD);
      %INC PGM(DALWBP);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;
      PROC DATASETS LIB=BNM NOLIST;
        DELETE SAVG&REPTMON&NOWK
               CURN&REPTMON&NOWK
               DEPT&REPTMON&NOWK;
      RUN;
      %INC PGM(FALWPBBD);
      %INC PGM(FALWPBBP);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;
      PROC DATASETS LIB=BNM NOLIST;
        DELETE FDWKLY;
      RUN;
      PROC COPY IN=BNM1 OUT=BNM;
        SELECT LOAN&REPTMON&NOWK
               ULOAN&REPTMON&NOWK;
      RUN;
      %INC PGM(LALWPBBP);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;
      PROC DATASETS LIB=BNM NOLIST;
        DELETE LOAN&REPTMON&NOWK
               ULOAN&REPTMON&NOWK;
      RUN;
      %INC PGM(KALWPIBP);
      %INC PGM(PBBRDAL1);
      %INC PGM(PBBELP);
      %INC PGM(PIBBRELP);
      %INC PGM(EIIWRDAL);
      %END;
  %ELSE %DO;
        %IF "&ALW" NE "&RDATE" %THEN
            %PUT THE RDAL1 EXTRACTION IS NOT DATED &RDATE;
        %IF "&LOAN" NE "&RDATE" %THEN
            %PUT THE LOAN EXTRACTION IS NOT DATED &RDATE;
        %IF "&DEPOSIT" NE "&RDATE" %THEN
            %PUT THE DEPOSIT EXTRACTION IS NOT DATED &RDATE;
        %IF "&FD" NE "&RDATE" %THEN
            %PUT THE FD EXTRACTION IS NOT DATED &RDATE;
        %IF "&KAPITI1" NE "&RDATE" %THEN
            %PUT THE KAPITI1 EXTRACTION IS NOT DATED &RDATE;
        %PUT THE JOB IS NOT DONE !!;
        %DO;
           DATA A;
              ABORT 77;
        %END;
  %END;
%MEND;
%PROCESS;
DATA _NULL_;
   FILE SFTP01;
   PUT @1 "PUT //SAP.PIBB.FISS.RDAL   ""RDAL WEEK&NOWK..TXT"""
       /  "PUT //SAP.PIBB.NSRS.RDAL   ""NSRS WEEK&NOWK..TXT"""
       ;
RUN;

//          '146 JALAN AMPANG','50450 KUALA LUMPUR')
//*
//DELETE   EXEC PGM=IEFBR14
//DD1      DD DSN=SAP.PBB.NPL.ARREARS.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//DD2      DD DSN=SAP.PBB.NPL.WOFFHP.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//DD3      DD DSN=SAP.PBB.NPL.HP.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//DD4      DD DSN=SAP.PBB.NPL.REPT.TEXT,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//*
//CREATE   EXEC PGM=IEFBR14
//CRT01    DD DSN=SAP.PBB.NPL.ARREARS.RPS,
//             DISP=(NEW,CATLG,DELETE),
//             DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//             SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//CRT02    DD DSN=SAP.PBB.NPL.WOFFHP.RPS,
//             DISP=(NEW,CATLG,DELETE),
//             DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//             SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//CRT03    DD DSN=SAP.PBB.NPL.HP.RPS,
//             DISP=(NEW,CATLG,DELETE),
//             DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//             SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//*
//EIFMNPLP EXEC SAS609
//*CONFIG  DD DISP=SHR,DSN=SYS3.SAS.V609.CNTL(BATCHXA)
//LOAN     DD DSN=SAP.PBB.MNILN(0),DISP=SHR
//*LNSAS    DD DSN=SAP.PBB.SASDATA,DISP=SHR
//BRHFILE  DD DSN=RBP2.B033.PBB.BRANCH,DISP=SHR
//WOFF     DD DSN=SAP.PBB.NPL.WOFF,DISP=SHR
//PGM      DD DSN=SAP.BNM.PROGRAM,DISP=SHR
//CAP      DD DSN=SAP.PBB.NPL.HP.SASDATA,DISP=SHR
//BNM      DD DSN=SAP.PBB.CCDTEMP(0),DISP=SHR
//OUTFILE  DD DSN=SAP.PBB.NPL.ARREARS.RPS,DISP=MOD
//OUTWOFF  DD DSN=SAP.PBB.NPL.WOFFHP.RPS,DISP=MOD
//OUTHP    DD DSN=SAP.PBB.NPL.HP.RPS,DISP=MOD
//SASLIST  DD DSN=SAP.PBB.NPL.REPT.TEXT,
//            DISP=(MOD,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=256,BLKSIZE=28160),
//            SPACE=(CYL,(5,2)),UNIT=SYSDA
//SYSIN    DD *

DATA REPTDATE;
  SET LOAN.REPTDATE;
  SELECT(DAY(REPTDATE));
    WHEN (8)  DO; WK = '1'; END;
    WHEN(15)  DO; WK = '2'; END;
    WHEN(22)  DO; WK = '3'; END;
    OTHERWISE DO; WK = '4'; END;
  END;
  CALL SYMPUT('NOWK',PUT(WK,$1.));
  CALL SYMPUT('RDATE', PUT(REPTDATE, DDMMYY8.));
  CALL SYMPUT('REPTYEAR', PUT(REPTDATE, YEAR4.));
  CALL SYMPUT('REPTMON', PUT(MONTH(REPTDATE), Z2.));
  CALL SYMPUT('REPTDAY', PUT(DAY(REPTDATE), Z2.));
RUN;
      /* %INC PGM(LNCCD006);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN; */

      /**  LOANS IN NPL REPORT - BY CAC **/
      %INC PGM(LNCCDN10);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;

      /**  MONTHLY DETAIL LISTING FOR NPL ACCOUNTS      **/
      /* %INC PGM(LNCCDR10);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;  */

      /* MTHLY DETAIL LISTING FOR NPL 3 MONTHS & ABOVE FOR ALL ACCT */
      %INC PGM(LNCCDQ10);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;

      /* MTHLY DETAIL LISTING FOR WRITTEN OFF HP 983 & 993 ACCT */
      %INC PGM(LNCCDW10);
      PROC DATASETS LIB=WORK KILL NOLIST; RUN;

      /* PROC DATASETS LIB=BNM NOLIST;
        DELETE LOANTEMP;
      RUN; */

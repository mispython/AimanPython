//*  PROGRAM TO PRINT THE IIS, SP1, SP2 INTO A TEXT FILE FOR RPS USE
//*  (SMR A264)
//*********************************************************************
//DELETE   EXEC PGM=IEFBR14
//DD1      DD DSN=SAP.PBB.NPL.IIS.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//*
//DD2      DD DSN=SAP.PBB.NPL.SP1.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//*
//DD3      DD DSN=SAP.PBB.NPL.SP2.RPS,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(CYL,(10,20))
//*
//CREATE   EXEC PGM=IEFBR14
//CRT01    DD DSN=SAP.PBB.NPL.IIS.RPS,
//            DISP=(NEW,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//            SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//CRT02    DD DSN=SAP.PBB.NPL.SP1.RPS,
//            DISP=(NEW,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//            SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA

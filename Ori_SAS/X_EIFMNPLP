//            DISP=(NEW,CATLG,DELETE),
//            DCB=(RECFM=FB,LRECL=134,BLKSIZE=13400),
//            SPACE=(CYL,(10,20),RLSE),UNIT=SYSDA
//*
//EIFMNPP1  EXEC SAS609
//RPSIIS    DD DSN=SAP.PBB.NPL.IIS.RPS,DISP=OLD
//RPSSP1    DD DSN=SAP.PBB.NPL.SP1.RPS,DISP=OLD
//RPSSP2    DD DSN=SAP.PBB.NPL.SP2.RPS,DISP=OLD
//NPL6      DD DSN=SAP.PBB.NPL.HP.SASDATA,DISP=SHR
//*NPOL6     DD DSN=SAP.PFB.NPOL6.SASDATA,DISP=SHR
//*NPIL6     DD DSN=SAP.PFB.NPIL6.SASDATA,DISP=SHR
//SYSIN     DD DSN=SAP.BNM.PROGRAM(EIFMNPP1),DISP=SHR
//*
//**********************************************************************
//*  TO PRINT IIS INTO RPS FORMAT
//**********************************************************************

PROC FORMAT;
  VALUE ORIMAT
    1         = '12'
    2         = '13'
    3         = '14'
    4 - 6     = '15'
    7 - 9     = '16'
    10 - 12   = '17'
    13 - HIGH = '20';

  VALUE KREMMTH
    LOW - 0   = '51'
      0 - 1   = '52'
      1 - 2   = '53'
      2 - 3   = '54'
      3 - 4   = '81'
      4 - 5   = '82'
      5 - 6   = '83'
      6 - 7   = '84'
      7 - 8   = '85'
      8 - 9   = '86'
      9 - 10  = '87'
     10 - 11  = '88'
     11 - 12  = '89'
     12 - HIGH = '60';

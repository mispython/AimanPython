*+--------------------------------------------------------------+
 |  PROGRAM : EIBQPBCR                                          |
 |  DATE    : 30.03.99                                          |
 |  PURPOSE : TO OFFER PB PREMIUM CLUB AUTOMATIC MEMBERSHIP     |
 |  REPORT  : LISTING OF ELIGIBLE DEPOSITORS FOR PB PREMIUM CLUB|
 |  NOTE    : INPUT ADDR.NAMELST IS OUTPUT FROM PGM EIBQADR2    |
 +--------------------------------------------------------------+;
OPTIONS YEARCUTOFF=1930 NOCENTER NODATE NONUMBER MISSING=0;
*;
%INC PGM(PBBELF);
*;
PROC FORMAT;
   VALUE SAPROD
      200   = 'PLUS'
      202   = 'YAA'
      203   = '50PLUS'
      212   = 'WISE'
      213   = 'PB SAVELINK'
      214   = 'MUDHARABAH BESTARI'
      150   = 'ACE NORMAL'
      151   = 'ACE STAFF'
      152   = 'ACE EXTERNAL'
      156   = 'PB CURRENTLINK'
      157   = 'PB CURRENTLINK EXT'
      204   = 'AL-WADIAH SA'
      100   = 'PLUS CURRENT'
      102   = 'PLUS CA EXT'
      160   = 'AL-WADIAH CA'
      162   = 'AL-WADIAH CA EXT'
      OTHER = 'UNKNOWN';
*;
   VALUE $SAPURP
      '1'     = 'PERSONAL'
      '2'     = 'JOINT'
      '5','6' = 'ON-BEHALF'
      OTHER   = 'UNKNOWN';
*;
DATA _NULL_;
   SET ADDR.REPTDATE;
   CALL SYMPUT('RDATE',PUT(REPTDATE,WORDDATX18.));
RUN;
*;
DATA NAMELST1;
   SET ADDR.NEW;
   DROP BRANCH;
   BRCH = PUT(BRANCH,Z3.)||'/'||PUT(BRANCH,BRCHCD.);
   RENAME BRCH=BRANCH;
*;
DATA NAMELST;
   SET ADDR.AUT;
   DROP BRANCH;
   BRCH = PUT(BRANCH,Z3.)||'/'||PUT(BRANCH,BRCHCD.);
   RENAME BRCH=BRANCH;
*;
DATA NAMELST3;
   SET ADDR.P50;
   DROP BRANCH;
   BRCH = PUT(BRANCH,Z3.)||'/'||PUT(BRANCH,BRCHCD.);
   RENAME BRCH=BRANCH;
*;
DATA NAMELST4;
   SET ADDR.HL;
   DROP BRANCH;
   BRCH = PUT(BRANCH,Z3.)||'/'||PUT(BRANCH,BRCHCD.);
   RENAME BRCH=BRANCH;
*;
DATA NAMELST5;
   SET ADDR.HP;
   DROP BRANCH;
   BRCH = PUT(BRANCH,Z3.)||'/'||PUT(BRANCH,BRCHCD.);
   RENAME BRCH=BRANCH;
*;
PROC SORT DATA=NAMELST1;  BY BRANCH NAME;
PROC SORT DATA=NAMELST;   BY BRANCH NAME;
PROC SORT DATA=NAMELST3;  BY BRANCH NAME;
PROC SORT DATA=NAMELST4;  BY BRANCH NAME;
PROC SORT DATA=NAMELST5;  BY BRANCH NAME;
*;
%LET TTL=LISTING OF DEPOSITORS FOR PB PREMIUM CLUB AS AT;
PROC PRINT DATA=NAMELST1 LABEL;
   FORMAT PURPOSE $SAPURP. PRODUCT SAPROD. YTDAVAMT COMMA14.2;
   LABEL BRANCH   = 'BRANCH CODE'
         ACCTNO   = 'ACCOUNT NO.'
         NAME     = 'CUSTOMER NAME'
         PURPOSE  = 'TYPE OF ACCOUNT'
         PRODUCT  = 'PRODUCT TYPE'
         YTDAVAMT = 'YTD AVERAGE BALANCE (RM)';
   VAR BRANCH ACCTNO NAME PURPOSE PRODUCT YTDAVAMT;
   TITLE1 'REPORT NO : EIBQPBCR';
   TITLE2 'PUBLIC BANK BERHAD';
   TITLE3 &TTL &RDATE;
*;
%LET TTL=LISTING OF ELIGIBLE DEPOSITORS FOR PB PREMIUM CLUB AS AT;
PROC PRINT DATA=NAMELST LABEL;
   FORMAT PURPOSE $SAPURP. PRODUCT SAPROD. YTDAVAMT COMMA14.2;
   LABEL BRANCH   = 'BRANCH CODE'
         ACCTNO   = 'ACCOUNT NO.'
         NAME     = 'CUSTOMER NAME'
         PURPOSE  = 'TYPE OF ACCOUNT'
         PRODUCT  = 'PRODUCT TYPE'
         YTDAVAMT = 'YTD AVERAGE BALANCE (RM)';
   VAR BRANCH ACCTNO NAME PURPOSE PRODUCT YTDAVAMT;
   TITLE1 'REPORT NO : EIBQPBCR';
   TITLE2 'PUBLIC BANK BERHAD';
   TITLE3 &TTL &RDATE;
*;
%LET TTL=LISTING OF 50 PLUS S/A FOR PB PREMIUM CLUB AS AT;
PROC PRINT DATA=NAMELST3 LABEL;
   FORMAT PURPOSE $SAPURP. PRODUCT SAPROD. YTDAVAMT COMMA14.2;
   LABEL BRANCH   = 'BRANCH CODE'
         ACCTNO   = 'ACCOUNT NO.'
         NAME     = 'CUSTOMER NAME'
         PURPOSE  = 'TYPE OF ACCOUNT'
         PRODUCT  = 'PRODUCT TYPE'
         YTDAVAMT = 'YTD AVERAGE BALANCE (RM)';
   VAR BRANCH ACCTNO NAME PURPOSE PRODUCT YTDAVAMT;
   TITLE1 'REPORT NO : EIBQPBCR';
   TITLE2 'PUBLIC BANK BERHAD';
   TITLE3 &TTL &RDATE;
*;
%LET TTL=LISTING OF HOUSING LOANS FOR PB PREMIUM CLUB AS AT;
PROC PRINT DATA=NAMELST4 LABEL;
   LABEL BRANCH   = 'BRANCH CODE'
         ACCTNO   = 'ACCOUNT NO.'
         NAME     = 'CUSTOMER NAME'
         LOANTYPE = 'LOAN TYPE'
         CUSTCODE = 'CUSTCODE'
         NETPROC  = 'NETPROC'
         APVLIMIT = 'APPROVED LIMIT (RM)';
   VAR BRANCH ACCTNO NAME LOANTYPE CUSTCODE NETPROC APVLIMIT;
   TITLE1 'REPORT NO : EIBQPBCR';
   TITLE2 'PUBLIC BANK BERHAD';
   TITLE3 &TTL &RDATE;
*;
%LET TTL=LISTING OF HIRE PURCHASE ACCTS FOR PB PREMIUM CLUB AS AT;
PROC PRINT DATA=NAMELST5 LABEL;
   LABEL BRANCH   = 'BRANCH CODE'
         ACCTNO   = 'ACCOUNT NO.'
         NAME     = 'CUSTOMER NAME'
         LOANTYPE = 'LOAN TYPE'
         CUSTCODE = 'CUSTCODE'
         NETPROC  = 'NETPROC'
         APVLIMIT = 'APPROVED LIMIT (RM)';
   VAR BRANCH ACCTNO NAME LOANTYPE CUSTCODE NETPROC APVLIMIT;
   TITLE1 'REPORT NO : EIBQPBCR';
   TITLE2 'PUBLIC BANK BERHAD';
   TITLE3 &TTL &RDATE;
